<h1>Imported Files</h1>
<%= render partial: 'partials/menu' %>
<table>
  <thead>
    <th>File Name</th>
    <th>Status</th>
    <th>Created at</th>
  </thead>
  <tbody>
    <% @imported_files.each do |c| %>
      <tr>
        <td><%= link_to c.file_name, imported_files_show_path(c) %></td>
        <td><%= c.status %></td>
        <td><%= c.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
      </tr>
    <% end %>
    
  </tbody>
</table>
<%= paginate @imported_files, left: 2, right: 2 %>